{
  "name": "@playkit-js/transcript-plugin",
  "version": "0.0.3",
  "license": "AGPL-3.0",
  "private": false,
  "bugs": {
    "url": "https://github.com/kaltura/playkit-js-transcript/issues"
  },
  "homepage": "https://github.com/kaltura/playkit-js-transcript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaltura/playkit-js-transcript.git"
  },
  "dependencies": {
    "@playkit-js-contrib/common": "^3.1.1",
    "@playkit-js-contrib/plugin": "^3.1.3",
    "@playkit-js-contrib/ui": "^3.1.3",
    "@types/classnames": "2.2.9",
    "@types/node": "12.12.21",
    "classnames": "2.2.6",
    "kaltura-typescript-client": "file:libs/kaltura-typescript-client-7.0.0-v20190324-101134.tgz",
    "preact": "^8.4.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@playkit-js-contrib/cli": "^1.0.0-beta.1",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "husky": "3.1.0",
    "tslint": "5.20.1",
    "typescript": "3.7.4"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "reset": "npm run clean && rm -rf node_modules",
    "build": "kcontrib build",
    "build:dev": "kcontrib build --dev",
    "serve": "kcontrib serve",
    "analyze": "npm run build && npx source-map-explorer dist/transcript.js",
    "lint": "tsc --noEmit && eslint ./src --ext .ts,.tsx",
    "lint:fix": "tsc --noEmit && eslint ./src --ext .ts,.tsx --fix",
    "husky:pre-commit": "lint-staged",
    "husky:commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "deploy:prepare": "kcontrib deploy --prepare",
    "deploy:publish-to-npm": "kcontrib deploy --publish",
    "deploy:next:prepare": "kcontrib deploy --prepare --prerelease next",
    "contrib:latest": "kcontrib contrib --type=latest",
    "contrib:next": "kcontrib contrib --type=next",
    "contrib:local": "kcontrib contrib --type=local"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src"
  ]
}
